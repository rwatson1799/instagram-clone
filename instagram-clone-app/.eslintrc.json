{
    "extends": "airbnb",
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"]}],
        "linebreak-style": 0,
        "no-unused-vars": 1,
        "no-shadow": "off",
        "import/prefer-default-export": 0
    }
}